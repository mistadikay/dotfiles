"*":
  "Remote-FTP":
    autoUploadOnSave: "only when connected"
  "advanced-open-file": {}
  "atom-gdb": {}
  "autocomplete-elixir":
    elixirPath: "/usr/local/bin/elixir"
  "autocomplete-modules":
    babelPluginModuleResolver: true
  core:
    disabledPackages: [
      "path-hyperclick"
      "go-debug"
      "minimap-highlight-selected"
    ]
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "coverage"
    ]
    telemetryConsent: "no"
    titleBar: "hidden"
    warnOnLargeFileLimit: 1
  docblockr:
    align_tags: "no"
    extend_double_slash: false
    param_description: false
    return_description: false
    simple_mode: true
  editor:
    fontFamily: "Hasklig-Light"
    fontSize: 16
    tabLength: 4
  "exception-reporting":
    userId: "5c8145d0-f8d3-afde-89d8-7b32c168ac94"
  "file-icons":
    coloured: false
    onChanges: true
  "file-types":
    "^Dockerfile.*": "source.dockerfile"
  "flow-ide": {}
  "git-blame":
    showHash: false
    showOnlyLastNames: true
  "go-config": {}
  "go-debug":
    panelInitialVisible: false
  "go-plus":
    currentPanelHeight: "25.846354166666668vh"
  gofmt:
    formatTool: "gofmt"
  "language-babel":
    autoIndentJSX: false
  "language-elm":
    elmOraclePath: "/Users/deniskoltsov/.config/fnm/bin/elm-oracle"
  "language-elm-make":
    elmOraclePath: "/Users/deniskoltsov/.config/fnm/bin/elm-make"
  linter:
    lintOnChangeInterval: 900
  "linter-eslint": {}
  "linter-flow":
    executablePath: "/Users/deniskoltsov/.config/fnm/bin/flow"
  "linter-golinter": {}
  "linter-stylelint":
    disableWhenNoConfig: true
  "linter-ui-default":
    panelHeight: 69
  "linter-write-good":
    severityLevel: "Info"
  "livestyle-atom":
    analyzer:
      notify: false
  minimap:
    disablePythonDocLinks: true
    plugins:
      "find-and-replace": true
      "find-and-replaceDecorationsZIndex": 0
      "git-diff": true
      "git-diffDecorationsZIndex": 0
      "highlight-selected": true
      "highlight-selectedDecorationsZIndex": 0
      pigments: true
      pigmentsDecorationsZIndex: 0
      selection: true
      selectionDecorationsZIndex: 0
  "one-dark-ui": {}
  "prettier-atom":
    formatOnSaveOptions:
      enabled: true
      showInStatusBar: true
    prettierOptions:
      parser: "flow"
      printWidth: 100
      singleQuote: true
    useEslint: true
  "project-manager":
    environmentSpecificProjects: true
  stylefmt:
    formatOnSave: true
  sunset:
    has_been_configured: true
  "tree-view":
    focusOnReveal: false
    hideIgnoredNames: true
    squashDirectoryNames: true
  welcome:
    showOnStartup: false
