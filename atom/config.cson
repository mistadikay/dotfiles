"*":
  "Remote-FTP":
    autoUploadOnSave: "only when connected"
  "advanced-open-file": {}
  core:
    disabledPackages: [
      "path-hyperclick"
      "Parinfer"
      "linter-stylelint"
      "linter-clojure"
      "language-clojure"
      "go-config"
      "go-debug"
      "go-get"
      "go-hyperclick"
      "autocomplete-go"
      "builder-go"
      "gofmt"
      "go-plus"
      "gometalinter-linter"
      "gorename"
      "tester-go"
    ]
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "coverage"
    ]
  editor:
    fontFamily: "Hasklig-Light"
    fontSize: 16
    tabLength: 4
  "exception-reporting":
    userId: "5c8145d0-f8d3-afde-89d8-7b32c168ac94"
  "file-icons":
    coloured: false
    onChanges: true
  fonts:
    fontFamily: "Source Code Pro Light"
  "go-config": {}
  "go-debug":
    panelInitialVisible: false
  linter:
    errorPanelHeight: 24
    lintOnFly: false
    showErrorInline: false
  "linter-stylelint":
    disableWhenNoConfig: true
  "livestyle-atom":
    analyzer:
      notify: false
  minimap:
    plugins:
      "find-and-replace": true
      "find-and-replaceDecorationsZIndex": 0
      "git-diff": true
      "git-diffDecorationsZIndex": 0
      "highlight-selected": true
      "highlight-selectedDecorationsZIndex": 0
      pigments: true
      pigmentsDecorationsZIndex: 0
      selection: true
      selectionDecorationsZIndex: 0
  "project-manager":
    environmentSpecificProjects: true
  "tree-view":
    focusOnReveal: false
    hideIgnoredNames: true
  welcome:
    showOnStartup: false
".ansi.text":
  editor:
    preferredLineLength: 100
    tabLength: 4
